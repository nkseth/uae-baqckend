{
  "kind": "collectionType",
  "collectionName": "addons",
  "info": {
    "name": "addons",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "cover": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "title": {
      "type": "string",
      "required": true
    },
    "buttontype": {
      "type": "string",
      "default": "SUBSCRIBE",
      "required": true
    },
    "currency": {
      "type": "string",
      "default": "AED",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true,
      "default": 0
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "addon1",
        "addon2"
      ],
      "required": true
    },
    "filteroption": {
      "type": "string",
      "required": true
    },
    "information": {
      "type": "text"
    },
    "priority": {
      "type": "integer",
      "required": true
    },
    "tenure": {
      "type": "enumeration",
      "enum": [
        "MONTH",
        "YEAR"
      ]
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "subscription",
        "addon"
      ],
      "default": "addon"
    },
    "headercolor": {
      "type": "string",
      "default": "#d1d7ff"
    }
  }
}
